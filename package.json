{
  "name": "@angular-architect/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "node ./node_modules/nx/bin/nx",
    "ng": "nx",
    "*** client ***": "",
    "admin-panel:start:development": "nx run admin-panel:serve:development --host 0.0.0.0 --poll 500 --no-cloud",
    "admin-panel:build:development": "nx run admin-panel:build:development --no-cloud",
    "admin-panel:build:production": "nx run admin-panel:build:production --no-cloud",
    "*** config ***": "",
    "nx:update": "nx migrate latest --no-cloud",
    "nx:run-migrations": "nx migrate --run-migrations --no-cloud",
    "ncu:update": "ncu -u -t minor",
    "ncu:update:interactive": "ncu -i --format group",
    "npm:audit": "npm audit",
    "npm:audit:fix": "npm audit fix --force",
    "*** lint ***": "",
    "lint": "nx run-many --all --target=lint --no-cloud",
    "lint:fix": "nx run-many --all --target=lint --fix=true --no-cloud",
    "*** stylelint ***": "",
    "stylelint": "node --max-old-space-size=8192 ./node_modules/stylelint/bin/stylelint.mjs apps/**/*.scss --config .stylelintrc",
    "stylelint:fix": "node --max-old-space-size=8192 ./node_modules/stylelint/bin/stylelint.mjs apps/**/*.scss --fix=true --config .stylelintrc",
    "*** test ***": "",
    "test:all": "nx run-many --all --target=lint --no-cloud"
  },
  "private": true,
